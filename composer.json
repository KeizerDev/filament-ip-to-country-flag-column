{
  "name": "mohammadhprp/filament-ip-to-country-flag-column",
  "description": "Display Country flag from IP address in your Filament tables",
  "keywords": [
    "mohammadhrpr",
    "laravel",
    "filament",
    "filament-ip-to-country-flag",
    "admin panel",
    "tables",
    "column"
  ],
  "homepage": "https://github.com/mohammadhprp/filament-ip-to-country-flag-column",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammadhprp",
      "email": "mohammadhprp@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "filament/filament": "^2.0|^3.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^7.0|^8.0",
    "phpunit/phpunit": "^9.5|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mohammadhprp\\IPToCountryFlagColumn\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mohammadhprp\\IPToCountryFlagColumn\\IPToCountryFlagColumnServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
